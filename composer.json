{
    "name": "godforhire/toast",
    "description": "Easy toast notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "godforhire"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "godforhire\\Toast\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "godforhire\\Toast\\ToastServiceProvider"
            ],
            "aliases": {
                "Toast": "godforhire\\Toast"
            }
        }
    }
}
